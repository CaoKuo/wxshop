.tabs .tabs_title {
  display: flex;
  border-bottom: 1rpx solid #ccc;
}
.tabs .tabs_title .title_item {
  display: flex;
  justify-content: center;
  align-content: center;
  flex: 1;
  padding: 15rpx 0;
}
.tabs .tabs_title .active {
  color: var(--themeColor);
  border-bottom: 5rpx solid currentColor;
}
